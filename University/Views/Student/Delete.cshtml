@model University.BL.Models.StudentVM


@{
    ViewBag.Title = "Delete Student";
}

 @section BootstrapSelect {
        <link href="~/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    }
<center class="block-header">
      <h2>Delete Student</h2>
</center>
<br />
<br />

<a class="btn btn-primary" asp-controller="Student" asp-action="index"> Back To List</a>


<br />
<br />

<form class="form-group" asp-controller="Student" asp-action="Delete">
    <label asp-for="Name">Name</label>
    <input type="text" class="form-control" readonly asp-for="Name" />
    <br />
    <br />

    <label asp-for="Email">Email</label>
    <input type="text" class="form-control" readonly asp-for="Email" />
    <br />

    <label asp-for="Adress">Address</label>
    <input type="text" class="form-control" readonly required asp-for="Adress" />
    <br />

    <label asp-for="HareDate">Hare Date</label>
    <input type="date" class="form-control"readonly asp-for="HareDate" />
    <br />

  

    <label asp-for="Notes">Notes</label>
    <textarea class="form-control" readonly asp-for="Notes" rows="4"></textarea>
   <br />
     
    <br />

    <select asp-for="CollegeId" asp-items="(SelectList)ViewBag.CollegeList">
        <option>Choose College</option>
    </select>
    <center>
        <input type="submit" value="Save" class="btn btn-info" />
   </center>

</form>

